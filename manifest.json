{
    "manifest_version": 3,
    "name": "Agora Trainer",
    "version": "1.0",
    "permissions": ["activeTab", "scripting"],
    "host_permissions": ["http://*/*","https://*/*"],
    "description": "An automated video learning tips generator",
    "background": {
        "service_worker": "src/Player.mjs"
    },
    "action": {
        "default_popup": "public/index.html"
    }
}