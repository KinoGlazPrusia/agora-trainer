{
    "manifest_version": 3,
    "name": "Agora Trainer",
    "version": "1.0",
    "permissions": ["activeTab", "scripting", "storage"],
    "host_permissions": ["http://*/*","https://*/*"],
    "description": "An automated video learning tips generator",
    "background": {
        "service_worker": "public/service.js",
        "type": "module"
    },
    "action": {
        "default_popup": "public/index.html"
    }
}